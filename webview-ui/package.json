{
  "name": "hyseim-build-profile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "set EXTEND_ESLINT=true && react-scripts start",
    "build": "node ./scripts/build-react-no-split.js",
    "test": "EXTEND_ESLINT=true react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/vscode-webview": "^1.57.1",
    "react-scripts": "^5.0.1",
    "rewire": "^6.0.0",
    "typescript": "^4.9.5"

  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-expressions": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
